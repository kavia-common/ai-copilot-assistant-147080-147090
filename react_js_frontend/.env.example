# Frontend Environment Variables (.env.example)
# Copy this file to `.env` in the same directory and fill in values as needed.

# REQUIRED: Base URL for the FastAPI backend (no trailing slash).
# The app defaults to http://localhost:3001 if not set.
REACT_APP_API_BASE_URL=http://localhost:3001

# OPTIONAL: Supabase credentials (only needed if enabling Supabase features).
# If not using Supabase, leave these blank or omit from your .env.
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=
